# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
VITE_SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Environment
VITE_ENVIRONMENT=development

# Analytics (optional)
VITE_GA_TRACKING_ID=your_google_analytics_id

# Error Tracking (optional)
VITE_SENTRY_DSN=your_sentry_dsn

# reCAPTCHA Configuration
VITE_RECAPTCHA_SITE_KEY=your_recaptcha_site_key_here
VITE_RECAPTCHA_SECRET_KEY=your_recaptcha_secret_key_here

# reCAPTCHA Testing Control (Development Only)
# Set to 'true' to force reCAPTCHA to work on localhost (for testing)
# Set to 'false' or remove to bypass reCAPTCHA on localhost (normal development)
VITE_FORCE_RECAPTCHA=false

# EmailJS Configuration (for MFA emails)
VITE_EMAILJS_SERVICE_ID=your_emailjs_service_id
VITE_EMAILJS_TEMPLATE_ID=your_emailjs_template_id
VITE_EMAILJS_PUBLIC_KEY=your_emailjs_public_key
